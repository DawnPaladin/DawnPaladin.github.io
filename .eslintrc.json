{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "eslint:recommended",
    "rules": {
        "indent": [
            2,
            "tab"
        ],
        "linebreak-style": [
            2,
            "windows"
        ],
        "semi": [
            1,
            "always"
        ],
		  "no-undef": [
			   0
		  ],
		  "no-unused-vars": [
			  0
		  ],
		  "comma-dangle": [
			  0
		  ],
		  "no-console": [
			  0
		  ]
    }
}
